CC = gcc -std=gnu99 -DUNIX -Dlinux `pkg-config --cflags gtk+-2.0` -Werror=missing-prototypes -Werror=implicit -Wreturn-type -Wunused -Wunused-parameter -Wuninitialized -O

DEFS = -DUNIX -Dlinux `pkg-config --cflags gtk+-2.0`

LIBS = `pkg-config --libs gtk+-2.0` -lm -lasound

RANLIB = ls
ICON =
